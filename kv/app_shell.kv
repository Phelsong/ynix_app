<AppShell>:
    BoxLayout:
        id: bar
        orientation: 'vertical'
        padding: '.5sp'
        size_hint: .06, 1
        canvas:
            Color:
                rgba: 1, 1, 1, .4
            Rectangle:
                size: self.size
                pos: self.pos
        Spinner:
            id: selector
            size_hint: 1, 0
            text: 'Welcome'
