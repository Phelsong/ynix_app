<AppShell>:
    BoxLayout:
        id: bar
        padding: '2sp'
        canvas:
            Color:
                rgba: 1, 1, 1, .6
            Rectangle:
                size: self.size
                pos: self.pos
        size_hint: 1, None
        height: '45sp'
        Spinner:
            id: selector
            size_hint: None, 1
            text: 'Welcome'
    ScreenManager:
        id: screen_manager
        Screen: 
            name: 'SplashScreen'
            SplashScreen:
        